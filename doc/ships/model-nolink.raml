#%RAML 1.0 DataType
type: object
properties:
    id: integer
    name : string
    "type" :
        type: string
        enum:
          - fighter
          - bomber
          - freighter
          - corvette
          - frigate
    frame : string
    slots:
        type: object[]
        default: []
    stats:
        type: object
        # TODO
    price:
        type: object[]
        default: []
    
